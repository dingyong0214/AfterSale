.device-info-outer {
  width: 100%;
  padding: 24rpx 25rpx;
  box-sizing: border-box;
}

.device-info {
  width: 100%;
  height: 210rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.device-info-title {
  width: 100%;
  height: 105rpx;
  font-size: 30rpx;
  color: #2f3030;
  font-weight: 500;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.err-describe-title {
  width: 100%;
  height: 85rpx;
  font-size: 30rpx;
  color: #2f3030;
  font-weight: 500;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.device-info-content {
  width: 100%;
  height: 72rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.row {
  width: 100%;
  display: flex;
  align-items: center;
  padding-left: 31rpx;
  box-sizing: border-box;
  height: 26rpx;
}

.row-sp {
  height: 26rpx;
  width: 100%;
  display: flex;
  align-items: center;
  padding-left: 31rpx;
  box-sizing: border-box;
}

.name {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #2f3030;
  padding-right: 29rpx;
  width: 115rpx;
  flex-shrink: 0;
}

.detail-area {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #2f3030;
  padding-right: 20rpx;
  padding-top: 6rpx;
  width: 115rpx;
  flex-shrink: 0;
}

.input-font {
  font-size: 26rpx;
  flex-shrink: 1;
  height: 30rpx;
}

.input-board2 {
  width: 75%;
  /* height: 80rpx; */
  height: 26rpx;
  font-size: 26rpx;
  line-height: 26rpx;
  /* padding-top: 10rpx; */
  box-sizing: border-box;
}

.read-value {
  font-size: 26rpx;
  line-height: 26rpx;
}

.custom-info {
  width: 100%;
  height: 430rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.custom-info-outer {
  width: 100%;
  padding: 0rpx 25rpx;
  box-sizing: border-box;
}

.custom-info-content {
  width: 100%;
  height: 260rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.err-describe-outer {
  width: 100%;
  padding: 24rpx 25rpx;
  box-sizing: border-box;
}

.err-describe {
  width: 100%;
  height: 270rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.input-board {
  width: 100%;
  height: 130rpx;
  border: 1rpx solid rgba(235, 235, 235, 1);
  border-radius: 6rpx;
  font-size: 26rpx;
  padding: 10rpx;
  box-sizing: border-box;
}

.input-outer-box {
  width: 100%;
  padding: 0 29rpx;
  box-sizing: border-box;
}

.place-holder-color {
  color: #bbbebf;
}

.btn-box {
  width: 100%;
  padding: 60rpx 125rpx 78rpx 125rpx;
  box-sizing: border-box;
}

.btn-self {
  font-size: 30rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 43rpx;
}

.choose-area {
  padding-left: 31rpx;
  box-sizing: border-box;
  display: flex;
  font-size: 26rpx;
  line-height: 26rpx;
  height: 26rpx;
  display: flex;
  align-items: center;
}

.choose-area-pl {
  padding-left: 360rpx;
}

.picker {
  width: 450rpx;
  /* height: 26rpx; */
  padding-left: 34rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
